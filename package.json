{
  "name": "alarisprime.com",
  "version": "3.0.0",
  "description": "The website for Alaris Prime LLC",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "npm-run-all clean --parallel build:*",
    "build:html": "metalsmith",
    "build:css": "node-sass --source-map-embed ./scss/entry.scss ./dist/css/bundle.css",
    "build:js": "webpack",
    "build:images": "cpx ./images/** ./dist/images/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/alarisprime/alarisprime.git"
  },
  "contributors": [
    "Saneef Ansari <s@alarisprime.com> (https://saneef.com/)",
    "Brajeshwar Oinam <b@alarisprime.com> (http://http://brajeshwar.me/)"
  ],
  "license": "UNLICENSED",
  "homepage": "https://alarisprime.com",
  "dependencies": {
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.12.3",
    "cpx": "^1.3.1",
    "metalsmith": "^2.1.0",
    "metalsmith-define": "^2.0.1",
    "metalsmith-in-place": "^1.4.3",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.7.0",
    "nunjucks": "^2.4.2",
    "rimraf": "^2.5.2",
    "watch": "^0.17.1",
    "webfontloader": "^1.6.24",
    "webpack": "^1.13.0"
  }
}
